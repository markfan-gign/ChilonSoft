    <script src="@Url.Content("~/Content/admin/scripts/jquery-1.4.2.min.js")" type="text/javascript"></script>
    <!--[if IE]><script language="javascript" type="text/javascript" src="@Url.Content("~/Content/admin/scripts/excanvas.min.js")"></script><![endif]-->
    <script src="@Url.Content("~/Content/admin/scripts/jquery-ui-1.8.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/admin/scripts/jquery.ui.selectmenu.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/admin/scripts/jquery.flot.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/admin/scripts/tiny_mce/tiny_mce.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/admin/scripts/tiny_mce/jquery.tinymce.js")" type="text/javascript"></script>
    <!-- scripts (custom) -->
    <script src="@Url.Content("~/Content/admin/scripts/smooth.js")" type="text/javascript"></script>
