    <script src="@Url.Content("~/Content/admin/scripts/smooth.dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/admin/scripts/smooth.autocomplete.js")" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            style_path = "../../Content/Admin/css/colors";
            $("#box-tabs, #box-left-tabs").tabs();
